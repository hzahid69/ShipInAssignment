{
  "name": "playwright-assignment",
  "version": "1.0.0",
  "description": "End-to-end test automation framework for automationexercise.com using Playwright with TypeScript and Page Object Model",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:e2e": "playwright test tests/automation-exercise-e2e.spec.ts",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:db": "playwright test --project=database",
    "test:db:users": "playwright test tests/database/user-crud.spec.ts --project=database",
    "test:db:products": "playwright test tests/database/product-crud.spec.ts --project=database",
    "test:db:orders": "playwright test tests/database/order-crud.spec.ts --project=database",
    "test:db:pom": "playwright test tests/database/database-pom.spec.ts --project=database",
    "test:grpc": "playwright test --project=grpc",
    "test:grpc:users": "playwright test tests/grpc/user-crud.spec.ts --project=grpc",
    "test:grpc:products": "playwright test tests/grpc/product-crud.spec.ts --project=grpc",
    "test:grpc:orders": "playwright test tests/grpc/order-crud.spec.ts --project=grpc",
    "test:grpc:pom": "playwright test tests/grpc/grpc-pom.spec.ts --project=grpc",
    "test:pom": "playwright test tests/database/database-pom.spec.ts tests/grpc/grpc-pom.spec.ts --project=database --project=grpc",
    "report": "playwright show-report",
    "install:browsers": "playwright install",
    "codegen": "playwright codegen https://automationexercise.com",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./proto/generated --ts_proto_opt=esModuleInterop=true ./proto/*.proto"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e",
    "typescript",
    "page-object-model",
    "database-testing",
    "grpc-testing"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.3",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.9",
    "dotenv": "^16.3.1",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "ts-proto": "^1.170.0",
    "protobufjs": "^7.2.5",
    "@types/google-protobuf": "^3.15.12"
  }
}
